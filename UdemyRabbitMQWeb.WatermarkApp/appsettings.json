{
  "ConnectionStrings": {
    "RabbitMQ": "amqp://guest:guest@localhost:5672"

  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "RabbitMQ",
        "Uri": "https://localhost:44383/health"
      }
    ],
    "EvaluationTimeOnSeconds": 30,
    "MinimumSecondsBetweenFailureNotifications": 60
    
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
